class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        numToIndex = {}
        for i, num in enumerate(nums):
            complement = target - num
            if complement in numToIndex:
                return [numToIndex[complement], i]
            numToIndex[num] = i
